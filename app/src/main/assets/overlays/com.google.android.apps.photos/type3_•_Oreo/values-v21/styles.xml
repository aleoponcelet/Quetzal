<resources>
    <style name="Platform.AppCompat.Light" parent="@android:style/Theme.Material.Wallpaper">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowElevation">0.5dip</item>
        <item name="android:backgroundTint">@*android:color/background_dark</item>
        <item name="android:textColorPrimaryInverse">@*android:color/accent_material_dark</item>
		<item name="android:navigationBarColor">@*android:color/background_dark</item>
		<item name="android:statusBarColor">@*android:color/transparent</item>
    </style>
    <style name="Base.Widget.AppCompat.Light.PopupMenu" parent="@*android:style/Widget.Material.PopupMenu" />
    <style name="GvrDialogTheme" parent="@*android:style/Theme.Material.Dialog" />
    <style name="Theme.Photos.Dialog" parent="@android:style/Theme.Material.Dialog.Alert">
        <item name="android:colorPrimary">@*android:color/material_grey_850</item>
        <item name="android:colorPrimaryDark">@*android:color/material_grey_850</item>
        <item name="android:colorAccent">@*android:color/accent_material_dark</item>
	</style>
    <style name="Platform.V21.AppCompat.Light" parent="@android:style/Theme.Material.Wallpaper">
        <item name="android:statusBarColor">@*android:color/transparent</item>
        <item name="android:navigationBarColor">@*android:color/background_dark</item>
	</style>
	<style name="UrawTheme" parent="@android:style/Theme.Material.Wallpaper">
        <item name="android:navigationBarColor">@*android:color/background_dark</item>
    </style>
