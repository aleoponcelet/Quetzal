<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Base.V27.Theme.YouTube.Light" parent="@*com.google.android.youtube:style/Base.V23.Theme.YouTube.Light">
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="android:navigationBarDividerColor">@*android:color/primary_dark_material_dark</item>
    </style>
    <style name="Base.V27.Theme.YouTube.Light.Dialog" parent="@*com.google.android.youtube:style/Base.V23.Theme.YouTube.Light.Dialog">
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="android:navigationBarDividerColor">@*android:color/primary_dark_material_dark</item>
    </style>
    <style name="Base.V27.Theme.YouTube.Share.DialogFragment" parent="@*com.google.android.youtube:style/Base.Theme.YouTube.Share.DialogFragment">
        <item name="android:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="android:navigationBarDividerColor">@*android:color/primary_dark_material_dark</item>
    </style>


    <style name="NavigationBar.Light.V27" parent="@*com.google.android.youtube:style/NavigationBar">
        <item name="*com.google.android.youtube:navigationBarColor">@*android:color/primary_dark_material_dark</item>
        <item name="*com.google.android.youtube:navigationBarControlsMode">light</item>
        <item name="*com.google.android.youtube:navigationBarDividerColor">@*android:color/primary_dark_material_dark</item>
        <item name="*com.google.android.youtube:navigationBarDividerSize">@*com.google.android.youtube:dimen/navigation_bar_divider_size_light</item>
    </style>
</resources>
